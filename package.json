    "jest": "23.2.0",
    "mini-css-extract-plugin": "^0.4.1",
    "npm-run-all": "4.1.3",
    "postcss": "6.0.23",
    "postcss-cssnext": "3.1.0",
    "postcss-loader": "2.1.5",
    "postcss-partial-import": "4.1.0",
    "prettier": "1.13.7",
