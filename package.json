    "presets": "./.babelrc.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run validate",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "release": {
    "analyzeCommits": "simple-commit-message"
  }
